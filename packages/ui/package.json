{
  "name": "@voyager-vue/ui",
  "version": "0.0.8",
  "description": "Vue component dependency visualizer UI",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./template": "./dist/template.html"
  },
  "files": [
    "dist"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/corrupt952/voyager.git",
    "directory": "packages/ui"
  },
  "homepage": "https://github.com/corrupt952/voyager#readme",
  "bugs": {
    "url": "https://github.com/corrupt952/voyager/issues"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "build": "vite build && vite build --config vite.visualizer.config.ts",
    "dev": "vite",
    "test": "vitest",
    "lint": "eslint src/**/*.{ts,tsx}",
    "format": "prettier --write src/**/*.{ts,tsx}"
  },
  "peerDependencies": {
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "dependencies": {
    "@dagrejs/dagre": "^2.0.0",
    "@voyager-vue/core": "workspace:*",
    "@xyflow/react": "^12.0.0"
  },
  "devDependencies": {
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@typescript-eslint/eslint-plugin": "^8.24.1",
    "@typescript-eslint/parser": "^8.24.1",
    "@vitejs/plugin-react": "^5.0.0",
    "eslint": "^9.20.1",
    "eslint-config-prettier": "^10.0.1",
    "prettier": "^3.5.1",
    "typescript": "^5.7.3",
    "vite": "^7.0.0",
    "vite-plugin-dts": "^3.7.3",
    "vitest": "^3.0.6"
  }
}
